new ModelHubDefinition("{{ Name }}", "{{ Path }}", 
  [
	{%- for c in Commands -%}
	  "{{ c.MethodName }}"{% unless forloop.last %}, {% endunless -%}
	{%- endfor -%}
  ],
  [
  	{% for q in Queries -%}
	  "{{ q.MethodName }}"{% unless forloop.last %}, {% endunless -%}
	{%- endfor %}
  ],
  [
  	{%- for e in Events -%}
	  "{{ e.Name }}"{% unless forloop.last %}, {% endunless -%}
	{%- endfor -%}
	]
  )
