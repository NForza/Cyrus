new ModelTypeDefinition(
	"{{ type.Name }}",  
	"{{ type.ClrName }}",  
	"{{ type.Description | escape }}",
	[
		{% for p in type.Properties %}
			{% include "Model-property", property: p %}{% unless forloop.last %}, {% endunless -%}
		{% endfor %}
	],
	[
		{% for p in type.Values %}
			"{{ p }}"{% unless forloop.last %}, {% endunless -%}
		{% endfor %}
	],
	{{ type.IsCollection }}, 
	{{ type.IsNullable }}
)
