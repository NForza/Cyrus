new ModelTypeDefinition(
	"{{ type.Name }}",  // Name
	"{{ type.Type }}", 
	[
		{% for p in Properties %}
			{% include "model-property", property: p %}{% unless forloop.last %}, {% endunless -%}
		{% endfor %}
	]
	{{ IsEnumerable }}, 
	{{ IsNullable }},
	[
		{% for st in SupportTypes %}
			{% include "model-type", type: st %}{% unless forloop.last %}, {% endunless -%}
		{% endfor %}
	])