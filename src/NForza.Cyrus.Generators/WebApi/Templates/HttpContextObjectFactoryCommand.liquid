    {% for cmd in Commands %}
    services.AddSingleton(new ObjectFactoryRegistration(typeof({{cmd.TypeName}}), 
        (ctx,obj) => 
        { 
            var cmd = obj is {{cmd.TypeName}} q ? q : new {{cmd.TypeName}}();
            {% for prop in query.Properties %}
            if (ctx.HasRouteOrQueryValueFor("{{ prop.Name }}", typeof({{ prop.Type}}), out var {{ prop.Name }}Value))
            {
              cmd.{{ prop.Name }} = ({{prop.Type}}){{ prop.Name}}Value;
            }
            {% endfor %}
            return cmd;
        }));
    {% endfor %}