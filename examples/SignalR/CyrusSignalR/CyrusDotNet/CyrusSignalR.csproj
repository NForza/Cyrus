<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <Nullable>enable</Nullable>
    <Cyrus-TypeScriptFolder>../CyrusSignalR-Angular/src/app/cyrus</Cyrus-TypeScriptFolder>
    <Cyrus-CleanTypeScriptFolder>true</Cyrus-CleanTypeScriptFolder>
    <EmitCompilerGeneratedFiles>true</EmitCompilerGeneratedFiles>
    <CompilerGeneratedFilesOutputPath>./obj/Debug/generated</CompilerGeneratedFilesOutputPath>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NForza.Cyrus" Version="1.0.321-beta-20251020-081334" />
    <PackageReference Include="NForza.Cyrus.MSBuild" Version="1.0.321-beta-20251020-081334" />
  </ItemGroup>

</Project>
